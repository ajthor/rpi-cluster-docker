#!/usr/bin/env bash

declare REL="${REL:-edge}"
declare MIRROR="${MIRROR:-http://nl.alpinelinux.org/alpine}"
declare REPO="${REPO:-$MIRROR/$REL/main}"
declare ARCH="${ARCH:-armhf}"

build() {

  [[ "$BUILDER_IMAGE" ]] || {
    BUILDER_IMAGE = "builder:alpine"
    docker build -t "$BUILDER_IMAGE" builder
  }



}

main() {

}

main "$@"
